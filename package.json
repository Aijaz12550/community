    "emotion": "^10.0.27",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "isomorphic-unfetch": "^3.0.0",
    "lscache": "^1.3.0",
    "next": "9.3.1",
    "next-offline": "^5.0.0",
    "next-redux-wrapper": "^5.0.0",
