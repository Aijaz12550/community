{
  "name": "communitysystem",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd --environments dev --file ./.env-cmdrc next dev",
    "build": "env-cmd --environments dev --file ./.env-cmdrc next build",
    "start": "env-cmd --environments dev --file ./.env-cmdrc next start",
    "pm2": "env-cmd --environments dev --file ./.env-cmdrc pm2 start server/index.js",
    "stop": "pm2 stop server/index.js",
    "server": "env-cmd --environments dev --file ./.env-cmdrc node server/index.js"
  },
